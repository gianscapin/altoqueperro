<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navgraph"
    app:startDestination="@id/logFragment">

    <fragment
        android:id="@+id/logFragment"
        android:name="com.ort.altoqueperro.fragments.LogFragment"
        android:label="log_fragment"
        tools:layout="@layout/log_fragment" >
        <action
            android:id="@+id/action_logFragment_to_signUpFragment2"
            app:destination="@id/signUpFragment" />
        <action
            android:id="@+id/action_logFragment_to_petFragment2"
            app:destination="@id/home_navgraph" />
    </fragment>
    <fragment
        android:id="@+id/signUpFragment"
        android:name="com.ort.altoqueperro.fragments.SignUpFragment"
        android:label="sign_up_fragment"
        tools:layout="@layout/sign_up_fragment" >
        <action
            android:id="@+id/action_signUpFragment_to_petFragment2"
            app:destination="@id/home_navgraph" />
    </fragment>
    <include app:graph="@navigation/home_navgraph" />
</navigation>